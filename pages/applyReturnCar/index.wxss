/**index.wxss**/
.flexRow {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.flexCol {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.beginOrderActive {
  opacity: 0.85;
}
.returnCar {
  width: 100%;
  position: relative;
}
.returnCar .goGetCar {
  position: fixed;
  z-index: 101;
  bottom: 60rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 690rpx;
  background: #fff;
  padding: 32rpx 36rpx 42rpx;
  border-radius: 8rpx;
  box-sizing: border-box;
}
