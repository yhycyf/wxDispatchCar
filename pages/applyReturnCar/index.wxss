/**index.wxss**/
.flexRow {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.flexCol {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.beginOrderActive {
  opacity: 0.85;
}
.returnCar {
  width: 100%;
  position: relative;
}
.returnCar .goGetCar {
  position: fixed;
  z-index: 101;
  bottom: 60rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 690rpx;
  background: #fff;
  padding: 32rpx 36rpx 42rpx;
  border-radius: 8rpx;
  box-sizing: border-box;
}
.returnCar .goGetCar .titleBox {
  width: 100%;
  justify-content: space-between;
}
.returnCar .goGetCar .titleBox .titleLeft {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
  line-height: 40rpx;
}
.returnCar .goGetCar .titleBox .titleRight .notice {
  width: 26rpx;
  height: 26rpx;
}
.returnCar .goGetCar .titleBox .titleRight .text {
  font-size: 20rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FB2020;
  margin-left: 2rpx;
  line-height: 26rpx;
}
.returnCar .goGetCar .content {
  width: 100%;
  align-items: flex-start;
}
.returnCar .goGetCar .content .list {
  width: 100%;
  height: 108rpx;
  justify-content: flex-start;
  position: relative;
}
.returnCar .goGetCar .content .list .iconBox {
  width: 40rpx;
}
.returnCar .goGetCar .content .list .iconBox .icon {
  width: 40rpx;
  height: 40rpx;
}
.returnCar .goGetCar .content .list .iconBox .iconTime {
  width: 28rpx;
  height: 28rpx;
}
.returnCar .goGetCar .content .list .iconBox .iconIphone {
  width: 32rpx;
  height: 32rpx;
}
.returnCar .goGetCar .content .list .iconBox .iconPerson {
  width: 28rpx;
  height: 28rpx;
}
.returnCar .goGetCar .content .list .text {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #AAAAAA;
  margin-left: 14rpx;
  opacity: 0.7;
}
.returnCar .goGetCar .content .list .iphoneText {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
  margin-left: 14rpx;
}
.returnCar .goGetCar .content .list .nameText {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
  margin-left: 14rpx;
}
.returnCar .goGetCar .content .list .tipsBox {
  margin-left: 34rpx;
  width: 282rpx;
  justify-content: flex-end;
  position: relative;
}
.returnCar .goGetCar .content .list .tipsBox .locationText {
  width: 282rpx;
  height: 60rpx;
  background: #1A1840;
  border-radius: 4rpx;
  justify-content: space-around;
}
.returnCar .goGetCar .content .list .tipsBox .locationText .text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  padding: 0 5rpx;
}
.returnCar .goGetCar .content .list .tipsBox .locationText .icon {
  width: 38rpx;
  height: 38rpx;
  opacity: 0.9;
}
.returnCar .goGetCar .content .list .tipsBox .triangle {
  position: absolute;
  top: 50%;
  transform: translateY(-50%) rotate(-90deg);
  left: -15rpx;
  width: 40rpx;
  height: 40rpx;
}
.returnCar .goGetCar .content .childBox {
  justify-content: space-between;
}
.returnCar .goGetCar .content .childBox .child {
  width: 50%;
  justify-content: flex-start;
  line-height: 40rpx;
}
.returnCar .goGetCar .content .line {
  width: 100%;
  height: 2rpx;
  background: #F0F0F0;
}
.returnCar .goGetCar .submit {
  width: 618rpx;
  height: 80rpx;
  background: #457CFB;
  border-radius: 47rpx;
  margin-top: 62rpx;
}
.returnCar .goGetCar .submit .text {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
