/**index.wxss**/
.addCarSuccess {
  padding-top: 80rpx;
  box-sizing: border-box;
}
.addCarSuccess .status {
  margin-bottom: 28rpx;
}
.addCarSuccess .status .center .image {
  width: 388rpx;
  height: 184rpx;
  margin-bottom: 24rpx;
}
.addCarSuccess .status .center .statusText {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
}
.addCarSuccess .status .center .tips {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #9F9F9F;
  margin-top: 16rpx;
}
.addCarSuccess .bottom {
  margin-top: 46rpx;
}
.addCarSuccess .bottom .beginOrder {
  width: 618rpx;
  height: 80rpx;
  background: #457CFB;
  border-radius: 40rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
