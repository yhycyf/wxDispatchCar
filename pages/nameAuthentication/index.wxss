/**index.wxss**/
.heard {
  margin: 48rpx 0  68rpx 66rpx;
}
.heard .title {
  font-size: 56rpx;
  font-weight: 300;
  color: #151515;
  margin-bottom: 16rpx;
}
.heard .marked {
  font-size: 22rpx;
  font-weight: 400;
  color: #AAAAAA;
}
.idSafety {
  width: 690rpx;
  margin: 0 auto;
}
.idSafety .nameAuthentication {
  width: 690rpx;
  height: 167rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 12rpx 2rpx rgba(200, 206, 211, 0.32);
  margin-bottom: 20rpx;
}
.idSafety .nameAuthentication .idIcon {
  width: 46rpx;
  height: 38rpx;
}
.idSafety .nameAuthentication .text {
  font-size: 24rpx;
  color: #777777;
}
.idSafety .nameAuthentication .text h5 {
  font-size: 32rpx;
  color: #151515;
}
.idSafety .nameAuthentication .goComplete {
  width: 110rpx;
  height: 54rpx;
  font-size: 24rpx;
  color: #fff;
  background-color: #457CFB;
  border-radius: 40rpx;
}
.idSafety .nameAuthentication .noComplete {
  width: 110rpx;
  height: 54rpx;
  font-size: 24rpx;
  color: #fff;
  background-color: #DDDEE3;
  border-radius: 40rpx;
}
.whatIsNot .text {
  position: fixed;
  bottom: 170rpx;
  left: 17%;
  font-size: 22rpx;
  color: #AAAAAA;
  text-align: center;
}
.whatIsNot .text .whatIsNotImage {
  position: fixed;
  bottom: 170rpx;
  left: 17%;
  width: 22rpx;
  height: 26rpx;
}
.whatIsNot .beginOrder {
  position: fixed;
  bottom: 70rpx;
  left: 17%;
  margin-left: -100rpx;
  width: 690rpx;
  height: 88rpx;
  background: #DDDEE3;
  border-radius: 45rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #AAAAAA;
}
